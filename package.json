{
  "name": "tb9c-bot",
  "version": "0.0.1",
  "description": "Discord bot for the retro fps server community TB9C",
  "scripts": {
    "start": "node ./src/index.js",
    "deploy-commands": "node ./src/deploy.js",
    "clear-logs": "del logs\\*.log",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "lint": "eslint ./src --ext .js,.mjs,.cjs",
    "fix": "eslint ./src --ext .js,.mjs,.cjs --fix",
    "update-handler": "npm uninstall @a06000208/handler && npm install github:06000208/handler",
    "update-framework": "npm uninstall @a06000208/discord-framework && npm install github:06000208/discord-framework",
    "update-git-pkg": "npm uninstall @a06000208/handler @a06000208/discord-framework && npm install github:06000208/handler github:06000208/discord-framework"
  },
  "author": "https://github.com/06000208/tb9c-bot/graphs/contributors",
  "homepage": "https://github.com/06000208/tb9c-bot",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.9.0"
  },
  "dependencies": {
    "@a06000208/discord-framework": "github:06000208/discord-framework",
    "@a06000208/handler": "github:06000208/handler",
    "@sapphire/discord-utilities": "^3.2.2",
    "@sapphire/ratelimits": "^2.4.9",
    "@sapphire/snowflake": "^3.5.3",
    "bufferutil": "^4.0.8",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "erlpack": "^0.1.4",
    "gamedig": "^5.0.0-beta.2",
    "lodash-es": "^4.17.21",
    "lowdb": "^7.0.1",
    "luxon": "^3.4.4",
    "pino": "^8.19.0",
    "toad-scheduler": "^3.0.1",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.21",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "pino-pretty": "^10.3.1"
  }
}
